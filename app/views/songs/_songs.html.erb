<table class="table table-stripped table-bordered table-hover">
  <thead>
  <tr>
    <th>Composition</th>
    <th>Durability</th>
  </tr>
  </thead>
  <tbody>
  <% album.songs.each do |song| %>
      <tr>
        <td><%= link_to song.name, artist_album_song_path(album.artist, album, song) %></td>
        <td><%= song.durability_in_time %></td>
      </tr>
  <% end %>
  </tbody>
</table>